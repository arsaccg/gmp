#content-inside
  .row
    .col-xs-12.col-sm-12.col-md-12.col-lg-12
      %h1.page-title.txt-color-blueDark{ "style" => "margin-bottom: 10px;"}
        %i.fa.fa-pencil-square-o.fa-fw
        Historico de Ordenes de Servicio
  %br
  #widget-grid
    .row
      %article.col-xs-12.col-sm-12.col-md-12.col-lg-12
        = render :partial => 'table'
%button.btn.btn-sm.btn-primary.btn-prev#backlist-service-order{type: "button"}
  %i.fa.fa-arrow-left>
  Retornar al listado de Ordenes de Servicio

:javascript
  
  $('#backlist-service-order').click(function(){
    load_url_ajax("#{logistics_order_of_services_path}", 'content',null, null, 'GET');
  });        
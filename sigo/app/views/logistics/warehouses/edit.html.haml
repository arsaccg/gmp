#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        = t('activerecord.actions.editm') + " " + t('activerecord.models.warehouse')
  #widget-grid
    .row
      %article.col-sm-12.col-md-12.col-lg-6
        = render :partial => 'form'
  %button.btn.btn-sm.btn-primary.btn-prev#backlist-cost{type: "button"}
    %i.fa.fa-arrow-left
      = t('activerecord.actions.back')

:javascript

  $('#backlist-cost').click(function(){
      load_url_ajax("#{logistics_warehouses_path}", 'content', null, null, 'GET');
  });
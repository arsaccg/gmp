#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-7
      %h2.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        = Budget.where(:id => @valorization.budget_id).first.description

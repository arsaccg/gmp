#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        Crear Grupo de Trabajo
  #widget-grid
    .row
      %article.col-sm-12.col-md-12.col-lg-6
        = render :partial => 'form'
  %button.btn.btn-sm.btn-primary.btn-prev#backlist-working-group{type: "button"}
    %i.fa.fa-arrow-left>
    Retornar al listado de Grupo de Trabajo

:javascript

  $('#backlist-working-group').click(function(){
      load_url_ajax("#{production_working_groups_path}", 'content', {company_id: '#{@company}'}, null, 'GET');
  });
.modal-dialog
  .modal-content
    .modal-header
      %button.close{"aria-hidden" => "true", "data-dismiss" => "modal", type: "button"}
        \Ã—
    .modal-body
      %h1.page-title.txt-color-blueDark Tabla de totales

      %table.table.table-bordered
        %thead
          %tr
            %th{'colspan' => 3, "style"=>"color:#5f5f5f"} Reporte Semanal
          %tr
            %th Semana
            %th Total HH
            %th Total TRAB.
        %tbody
        - @totalT.each do |th|
          %tr
            %th{ 'style' => 'font-weight: normal;text-align:left;vertical-align: middle' }= th[0].to_s
            %th{ 'style' => 'font-weight: normal;text-align:left;vertical-align: middle' }= th[1].to_s + " hh."         
            %th{ 'style' => 'font-weight: normal;text-align:left;vertical-align: middle' }= th[2].to_s + " trab."  
      %footer
        %button.btn.btn-info{"data-dismiss" => "modal", type: "button"}
          Salir

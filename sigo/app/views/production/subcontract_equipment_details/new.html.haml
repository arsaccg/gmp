#content-inside
  .row
    .col-xs-12.col-sm-12.col-md-12.col-lg-12
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        Crear Parte de Equipo
  #widget-grid
    .row
      %article.col-sm-12.col-md-12.col-lg-12
        = render :partial => 'form'

  %button.btn.btn-sm.btn-primary.btn-prev#backlist{type: "button"}
    %i.fa.fa-arrow-left>
    Retornar al listado de Partes de Equipos

:javascript

  $('#backlist').click(function(){
      load_url_ajax("#{production_subcontract_equipment_details_path}", 'content', {company_id: '#{@company}', subcontract: '#{@subcontract}'}, null, 'GET');
  });
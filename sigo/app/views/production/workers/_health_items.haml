%tr(class="health_center_item"){id: "tr" + @reg_n.to_s}
  %td.bank-id(style="display:none")
    = hidden_field_tag 'worker[worker_healths_attributes][' + @reg_n.to_s + '][health_center_id]', @id_health_center.to_i
  %td.col-2
    %div
      = text_field_tag 'worker[worker_healths_attributes][' + @reg_n.to_s + '][health_regime]', @health_regime.to_s , class: "form-control quantity", :required => true, title:'No puede dejar este campo vacio'
  %td.col-2
    = @enterprise.to_s
  %td.col-2
    %div
      = text_field_tag 'worker[worker_healths_attributes][' + @reg_n.to_s + '][start_date]', @start_date , class: "form-control quantity"
  %td.col-2
    %div
      = text_field_tag 'worker[worker_healths_attributes][' + @reg_n.to_s + '][end_date]', @end_date , class: "form-control quantity"
  %td{:style => "width: 1%;"}
    %a{href: "javascript:delete_item('"+@reg_n.to_s+"')"}(class="btn") X
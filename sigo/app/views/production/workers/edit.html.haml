#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        Editar Trabajador
  #widget-grid
    .row
      %article.col-sm-12.col-md-12.col-lg-11
        = render :partial => 'form'
  %button.btn.btn-sm.btn-primary.btn-prev#backlist-sector{type: "button"}
    %i.fa.fa-arrow-left>
    Retornar al Listado de Trabajadores

:javascript

  $('#backlist-sector').click(function(){
      load_url_ajax("#{production_workers_path}", 'content', {company_id: '#{@company}'}, null, 'GET');
  });
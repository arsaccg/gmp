#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw 
        Tareo de Obreros Semanal

  .row
    .well
      .input-group
        .col-sm-5
          %label.input{for: "start_date"}
            %input#start-date-input.string.optional.form-control{:name => "start_date", :type => "date"}
        .col-sm-5
          %label.input{for: "end_date"}
            %input#end-date-input.string.optional.form-control{:name => "end_date", :type => "date"}
        .col-sm-2
          %button#show-table.btn.btn-primary.btn-default{:type => 'button'}
            %i.fa.fa-search
            Buscar
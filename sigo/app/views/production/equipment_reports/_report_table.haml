          .widget-body
            %table.table.table-bordered
              %thead
                %tr
                  %th #
                  %th Nombre Tarea/Fase
                  %th H.Efectivas
                  %th Combustible (gln)
                  %th Ratio real (gln/hr)
                  %th Valor Teórico (gln/hr)
              %tbody
                - data[1].each do |value|
                  - value['data'].each do |data|
                    %tr
                      %th{ 'style' => 'font-weight: normal;' }
                        = data[0]
                      %th{ 'style' => 'font-weight: normal;' }
                        = data[1]
                      %th{ 'style' => 'font-weight: normal;' }
                        = sprintf('%.2f',data[2].to_f)
                      %th{ 'style' => 'font-weight: normal;' }
                        = sprintf('%.2f',data[3].to_f)
                      %th{ 'style' => 'font-weight: normal;' }
                        = sprintf('%.2f',data[4].to_f)
                      %th{ 'style' => 'font-weight: normal;' }
                        = sprintf('%.2f',@theoretical_value.to_f)
                  %tr
                    %th{'colspan' => 2} Total
                    %th
                      = sprintf('%.2f',value['hours'].to_f)
                    %th
                      = sprintf('%.2f',value['fuel'].to_f)
                    %th
                      = sprintf('%.2f',value['ratio'].to_f)
                    %th
                      = sprintf('%.2f',@theoretical_value.to_f)

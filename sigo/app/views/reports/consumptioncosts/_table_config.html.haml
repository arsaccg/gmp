            %strong VALOR GANADO
          %td{:style => 'text-align:center;'}
            %strong COSTO REAL
          %td{:style => 'text-align:center;'}
            %strong META
      - arr = ["CD","GG","GS"]
      -i = 0
      - @treeOrders.each do |tree_order|
        %tr
          %td
            %strong= tree_order.name.upcase
          - accum = ConsumptionCost.get_total_father(@for_acumulated_end_date,@cost_center_id,arr[i],@array_order_filters, @array_columns_delivered_sum)
          -i+=1
          %td
            = accum[0].to_f.round(4).round(2)
          %td
            = accum[1].to_f.round(4).round(2)
          %td
            = accum[2].to_f.round(4).round(2)
          %td
            = accum[3].to_f.round(4).round(2)
          %td
            = accum[4].to_f.round(4).round(2)

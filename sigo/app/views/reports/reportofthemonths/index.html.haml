#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw
        Reporte Semanal
  %div
    .jarviswidget-editbox
    .widget-body.no-padding
      .widget-body-toolbar
      %table#dt_basic.table.table-striped.table-bordered.table-hover
        %thead
          %tr
            %th Actual
            %th Acumulado
        %tbody
          %tr
            %th= (@partwork+@partequipment+@partpeople+@orderofservice+@scvaluations).round(2)
            %th= (@partwork2+@partequipment2+@partpeople2+@orderofservice2+@scvaluations2).round(2)
          %tr
            %th= @partwork.round(2)
            %th= @partwork2.round(2)
          %tr
            %th= @partequipment.round(2)
            %th= @partequipment2.round(2)
          %tr
            %th= @partpeople.round(2)
            %th= @partpeople2.round(2)
          %tr
            %th= @orderofservice.round(2)
            %th= @orderofservice2.round(2)
          %tr
            %th= @scvaluations.round(2)
            %th= @scvaluations2.round(2)
#content-inside
  .row
    .col-xs-12.col-sm-7.col-md-7.col-lg-4
      %h1.page-title.txt-color-blueDark
        %i.fa.fa-pencil-square-o.fa-fw
        Reporte Semanal
  %div
    .jarviswidget-editbox
    .widget-body.no-padding
      .widget-body-toolbar
      %table#dt_basic.table.table-striped.table-bordered.table-hover
        %thead
          %tr
            %th{'colspan' => 2, "style"=>"text-align:center"} REAL
          %tr
            %th{"style"=>"text-align:center;color:#38C0FB"} Mes Actual
            %th{"style"=>"text-align:center"} Acumulado
        %tbody
          %tr
            %th{"style"=>"text-align:center"}= (@partwork+@partequipment+@partpeople+@orderofservice+@scvaluations).round(2)
            %th{"style"=>"text-align:center"}= (@partwork2+@partequipment2+@partpeople2+@orderofservice2+@scvaluations2).round(2)
          %tr
            %th{"style"=>"text-align:center"}= @partwork.round(2)
            %th{"style"=>"text-align:center"}= @partwork2.round(2)
          %tr
            %th{"style"=>"text-align:center"}= @partequipment.round(2)
            %th{"style"=>"text-align:center"}= @partequipment2.round(2)
          %tr
            %th{"style"=>"text-align:center"}= @partpeople.round(2)
            %th{"style"=>"text-align:center"}= @partpeople2.round(2)
          %tr
            %th{"style"=>"text-align:center"}= @orderofservice.round(2)
            %th{"style"=>"text-align:center"}= @orderofservice2.round(2)
          %tr
            %th{"style"=>"text-align:center"}= @scvaluations.round(2)
            %th{"style"=>"text-align:center"}= @scvaluations2.round(2)